      "alternativas": [
        {
          "opcion": "A",
          "texto": "4.0–5.0"
        },
        {
          "opcion": "B",
          "texto": "5.0–8.0"
        },
        {
          "opcion": "C",
          "texto": "6.5–9.5"
        },
        {
          "opcion": "D",
          "texto": "3.0–4.5"
        },
        {
          "opcion": "E",
          "texto": "7.5–9.0"
        }
      ],
      "respuesta_correcta": "B",
      "explicacion": "El pH urinario normal oscila entre 5.0 y 8.0, dependiendo de la dieta y el metabolismo."
    },
    {
