  "preguntas": [
    {
      "numero": "1",
      "texto": "¿Cuál es el principal objetivo del examen de orina completa en pacientes con diabetes mellitus?",
      "alternativas": [
        {
          "opcion": "A",
          "texto": "Evaluar función pancreática"
        },
        {
          "opcion": "B",
          "texto": "Detectar proteinuria o glucosuria temprana"
        },
        {
          "opcion": "C",
          "texto": "Medir presión arterial"
        },
        {
          "opcion": "D",
          "texto": "Cuantificar colesterol"
        },
        {
          "opcion": "E",
          "texto": "Evaluar función hepática"
        }
      ],
      "respuesta_correcta": "B",
      "explicacion": "El examen de orina completa permite detectar glucosuria, proteinuria o cetonuria, signos precoces de descompensación o nefropatía diabética."
    },
    {
      "numero": "2",
      "texto": "La presencia de glucosa en orina (glucosuria) en un paciente diabético indica:",
      "alternativas": [
        {
          "opcion": "A",
          "texto": "Hipoglucemia"
        },
        {
          "opcion": "B",
          "texto": "Nivel normal de glucemia"
        },
        {
          "opcion": "C",
          "texto": "Exceso de insulina"
        },
        {
          "opcion": "D",
          "texto": "Glucemia sobre el umbral renal"
        },
        {
          "opcion": "E",
          "texto": "Daño hepático"
        }
      ],
      "respuesta_correcta": "D",
      "explicacion": "La glucosuria aparece cuando la glucemia supera el umbral renal de 180 mg/dL."
    },
    {
      "numero": "3",
      "texto": "La detección de cuerpos cetónicos en orina es un signo de:",
      "alternativas": [
        {
          "opcion": "A",
          "texto": "Hipoglucemia"
        },
        {
          "opcion": "B",
          "texto": "Cetoacidosis diabética"
        },
        {
          "opcion": "C",
          "texto": "Insuficiencia renal aguda"
        },
        {
          "opcion": "D",
          "texto": "Hepatopatía crónica"
        },
        {
          "opcion": "E",
          "texto": "Deshidratación leve"
        }
      ],
      "respuesta_correcta": "B",
      "explicacion": "Los cuerpos cetónicos en orina indican lipólisis aumentada, característica de la cetoacidosis diabética."
    },
    {
      "numero": "4",
      "texto": "La presencia persistente de proteínas en orina sugiere:",
      "alternativas": [
        {
          "opcion": "A",
          "texto": "Cistitis"
        },
        {
          "opcion": "B",
          "texto": "Nefropatía diabética"
        },
        {
          "opcion": "C",
          "texto": "Insuficiencia hepática"
        },
        {
          "opcion": "D",
          "texto": "Deshidratación"
        },
        {
          "opcion": "E",
          "texto": "Hipoglucemia"
        }
      ],
      "respuesta_correcta": "B",
      "explicacion": "La proteinuria persistente es un signo temprano de nefropatía diabética y daño glomerular."
    },
    {
      "numero": "5",
      "texto": "El hallazgo de microalbuminuria en orina se considera:",
      "alternativas": [
        {
          "opcion": "A",
          "texto": "Normal"
        },
        {
          "opcion": "B",
          "texto": "Indicador temprano de daño renal"
        },
        {
          "opcion": "C",
          "texto": "Secuela de infección urinaria"
        },
        {
          "opcion": "D",
          "texto": "Resultado de deshidratación"
        },
        {
          "opcion": "E",
          "texto": "Efecto de diuréticos"
        }
      ],
      "respuesta_correcta": "B",
      "explicacion": "La microalbuminuria refleja daño glomerular incipiente y es el primer signo de nefropatía diabética."
    },
    {
      "numero": "6",
